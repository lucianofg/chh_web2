{{#if item}}
    <h1>Item "{{item}}" salvo com sucesso!</h1>
{{else}}
    <div class="container-sm">
        <div class="alert alert-danger" role="alert">
            <h3>Erro em salvar item</h3>
            {{#if error}}
                <pre>{{error}}</pre>
            {{/if}}
        </div>
    </div>
{{/if}}
<a href='/concurso/{{concurso_id}}/view'><button class="btn btn-primary btn-lg">Voltar a página do concurso</button></a>
<br>
<a href='/'><button class="btn btn-secondary">Voltar a página inicial</button></a>
